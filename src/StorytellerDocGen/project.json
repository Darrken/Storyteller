{
  "version": "1.0.0-alpha-*",
  "title": "StorytellerDocGen",
  "description": "Living technical documentation authoring for .Net and Javascript projects",
  "language": "en-US",
  "authors": [
    "Jeremy D. Miller"
  ],
  "packOptions": {
    "projectUrl": "http://storyteller.github.io/documentation/docs/",
    "tags": [
      "documentation static website generator"
    ],
    "licenseUrl": "https://github.com/storyteller/storyteller/raw/master/LICENSE.TXT",
    "repository": {
      "type": "git",
      "url": "git://github.com/storyteller/storyteller"
    }
  },
  "name": "dotnet-stdocs",
  "buildOptions": {
    "emitEntryPoint": true,
    "compile": [
      "**/*.cs",
      "../CommonAssemblyInfo.cs"
    ],
    "embed": [ "../ST/topics.js", "Runner/WebsocketsRefresh.txt" ],
    "outputName": "dotnet-stdocs"
  },

  "dependencies": {
    "Oakton": "1.0.0",
    "Markdig": "0.8.5",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Microsoft.AspNetCore.Hosting": "1.0.0",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.AspNetCore.WebSockets.Server": "0.1.0",
    "HtmlTags.AspNetCore": "4.1.0",
    "Baseline": "1.1.0",
    "StructureMap": "4.4.0",
    "StoryTeller": { "target": "project" },
    "System.AppContext": "4.1.0"
  },

  "frameworks": {
    "net46": {

    },
    "netcoreapp1.0": {
      "dependencies": {
        "System.Runtime.Extensions": "4.1.0",
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": "dnxcore50"
    }
  }
}
